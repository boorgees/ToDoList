{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/todolista/browser",
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/.*",
      "dest": "/index.html"
    }
  ]
} 